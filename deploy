#!/bin/sh

# Create symlinks
ln -s "$PWD/bin"			"$HOME/.local"
ln -s "$PWD/gitconfig"		"$HOME/.gitconfig"
ln -s "$PWD/profile"		"$HOME/.profile"
ln -s "$PWD/sfeedrc"		"$HOME/.sfeed"
ln -s "$PWD/fish"			"$XDG_CONFIG_HOME"
ln -s "$PWD/mpd.conf"		"$XDG_CONFIG_HOME/mpd"
ln -s "$PWD/ncmcpp"			"$XDG_CONFIG_HOME"
ln -s "$PWD/nvim"			"$XDG_CONFIG_HOME"
ln -s "$PWD/sxhkdrc"		"$XDG_CONFIG_HOME/sxhkd"
ln -s "$PWD/youtube-dl"		"$XDG_CONFIG_HOME"
ln -s "$PWD/zathura"		"$XDG_CONFIG_HOME"
